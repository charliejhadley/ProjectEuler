Cell["Solution to Problem", "Section",
 CellChangeTimes->{{3.6355314833618937`*^9, 3.635531486784003*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"AbsoluteTiming", "[", 
  RowBox[{
   RowBox[{"i", "=", "1"}], ";", "\[IndentingNewLine]", 
   RowBox[{"crnt", "=", "0"}], ";", "\[IndentingNewLine]", 
   RowBox[{"less4Million", "=", 
    RowBox[{
     RowBox[{"Reap", "[", 
      RowBox[{"While", "[", 
       RowBox[{
        RowBox[{"crnt", "<", 
         RowBox[{"4", "*", 
          RowBox[{"10", "^", "6"}]}]}], ",", 
        RowBox[{
         RowBox[{"crnt", "=", 
          RowBox[{"Sow", "[", 
           RowBox[{"fib", "[", "i", "]"}], "]"}]}], ";", 
         RowBox[{"i", "++"}]}]}], "]"}], "]"}], "[", 
     RowBox[{"[", 
      RowBox[{"2", ",", "1", ",", 
       RowBox[{";;", 
        RowBox[{"-", "2"}]}]}], "]"}], "]"}]}], ";", "\n", 
   RowBox[{"Total", "[", 
    RowBox[{"Select", "[", 
     RowBox[{"less4Million", ",", "EvenQ"}], "]"}], "]"}]}], "]"}]], "Input",
 CellChangeTimes->{{3.6355310689576144`*^9, 3.6355310728078756`*^9}, {
